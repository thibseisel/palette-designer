<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RatioImageView">
        <!-- The image aspect ratio encoded as a string. -->
        <attr name="aspectRatio" format="string" />
        <attr name="aspectRatioSide" format="enum">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
    </declare-styleable>
</resources>